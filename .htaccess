# Turn rewrite engine on
RewriteEngine on

#POST do LOGIN 
#RewriteRule ^auth/([0-9a-zA-z$./]+) restful/restful.php?url=auth&token=$1 [L,QSA]
RewriteRule ^auth index.php?url=auth [L,QSA]
